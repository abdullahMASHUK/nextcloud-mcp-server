{
  "name": "nextcloud-mcp-server",
  "version": "1.0.5",
  "description": "Professional NextCloud MCP server for file operations and sharing",
  "author": "Abdullah MASHUK <abdullah.devs@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    }
  },
  "bin": {
    "nextcloud-mcp-server": "build/index.js"
  },
  "files": [
    "build/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node build/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf build",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "nextcloud",
    "webdav",
    "file-sharing",
    "file-management",
    "typescript",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdullahMASHUK/nextcloud-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/abdullahMASHUK/nextcloud-mcp-server/issues"
  },
  "homepage": "https://github.com/abdullahMASHUK/nextcloud-mcp-server#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/abdullahMASHUK"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "fast-xml-parser": "^5.2.5",
    "form-data": "^4.0.1",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.41.0"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapping": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "roots": [
      "<rootDir>/src",
      "<rootDir>/__tests__"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types.ts"
    ]
  }
}
